### Header whitespace test
## This test demonstrates the behaviour of whitespace stripping in doc comment headers.

let { doc } = import symbols;

fn f_2() {
	# no whitespace before header
	###F
}

fn f_1() {
	# whitespace before header
	### F
};

fn f_1() {
	# lots of whitespace before header
	###      F
};

f_1[doc].header == f_2[doc].header == f_3[doc].header == "F";
